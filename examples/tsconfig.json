{
    "compilerOptions": {
        "module": "esnext",
        "target": "es2020",
        "moduleResolution": "node",
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "importsNotUsedAsValues": "error",
    },
}
